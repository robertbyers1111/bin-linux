#!/bin/sh

nano9 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano8 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano7 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9][0-9][0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano6 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9][0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano5 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano4 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano3 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9][0-9]\)[0-9]*$/-\1/g'
}

nano2 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9][0-9]\)[0-9]*$/-\1/g'
}

nano1 ()
{
  date +%y%m%d-%H%M%S-%N | sed 's/-\([0-9]\)[0-9]*$/-\1/g'
}

noNano ()
{
  date +%y%m%d-%H%M%S
}

if [ $# -ne 1 ]; then
  nano3
else
  case $1 in
    0) noNano
    ;;
    1|2|3|4|5|6|7|8|9) nano$1
    ;;
    *) nano3
    ;;
  esac
fi

